<table class="table table-hover table-light">
  <thead>
    <tr>
      <th>Firstname</th>
      <th>Lastname</th>
      <th>Email</th>
    </tr>
  </thead>
  <tbody>
  	<% users.each do |user| %>  
    	<tr>
	      <td><%= user.first_name %></td>
	      <td><%= user.last_name %></td>
	      <td><%= user.email %></td>
    	</tr>
    <% end %>
  </tbody>
</table>
